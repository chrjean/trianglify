{
  "name": "trianglify",
  "version": "1.2.0",
  "description": "Trianglify is a javascript library for generating colorful triangle meshes that can be used as SVG images and CSS backgrounds.",
  "main": "lib/trianglify.js",
  "dependencies": {
    "chroma-js": "^0.6.3",
    "colors": "^1.3.2",
    "delaunator": "^1.0.5",
    "jsdom": "^7.0.2",
    "seedrandom": "^2.4.4"
  },
  "optionalDependencies": {
    "canvas": "^1.3.0"
  },
  "directories": {
    "example": "examples"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/qrohlf/trianglify.git"
  },
  "keywords": [
    "svg",
    "d3.js",
    "visualization"
  ],
  "author": "Quinn Rohlf <qr@qrohlf.com>",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/qrohlf/trianglify/issues"
  },
  "homepage": "https://github.com/qrohlf/trianglify",
  "scripts": {
    "test": "mocha test/test.js",
    "start": "gulp",
    "postinstall": "node scripts/postinstall.js"
  },
  "browser": {
    "jsdom": false,
    "canvas": false
  },
  "devDependencies": {
    "browserify": "^16.2.2",
    "chai": "^2.1.0",
    "del": "^3.0.0",
    "gulp": "^4.0.0",
    "gulp-babel-minify": "^0.2.0",
    "gulp-bump": "^3.1.1",
    "gulp-filter": "^5.1.0",
    "gulp-git": "^0.5.2",
    "gulp-jshint": "^2.1.0",
    "gulp-mocha": "^5.0.0",
    "gulp-rename": "^1.4.0",
    "gulp-sourcemaps": "^1.12.1",
    "gulp-tag-version": "^1.3.1",
    "jshint-stylish": "^0.4.0",
    "mocha": "^5.2.0",
    "node-notifier": "^5.2.1",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^1.1.2"
  },
  "npmName": "trianglify",
  "npmFileMap": [
    {
      "basePath": "dist",
      "files": [
        "trianglify.min.js"
      ]
    }
  ]
